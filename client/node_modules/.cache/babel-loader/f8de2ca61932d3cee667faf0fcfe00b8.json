AA,gBAZY,eAgBZ;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,gBAhBY,eAoBZ;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,gBApBY;AAAA;AAAA;AAAA;AAAA;AAAA;AAFA;AAAA;AAAA;AAAA;AAAA,YADJ,eAiCI;AAAA,6BACA,QAAC,GAAD;AAAK,QAAA,SAAS,EAAEA,GAAhB;AAAqB,QAAA,QAAQ,EAAC,UAA9B;AAAA,kBACK,CAAC;AAAEI,UAAAA;AAAF,SAAD,kBAAa;AAAQ,UAAA,OAAO,EAAEA,KAAjB;AAAwB,UAAA,KAAK,EAAE;AAACC,YAAAA,eAAe,EAAC,SAAjB;AAA2BC,YAAAA,KAAK,EAAC;AAAjC,WAA/B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AADlB;AAAA;AAAA;AAAA;AAAA;AADA;AAAA;AAAA;AAAA;AAAA,YAjCJ;AAAA,kBAFJ;AA8CH,CAhDD;;KAAMJ,G;AAiDN,eAAeA,GAAf","sourcesContent":["\r\nimport React from 'react';\r\n\r\nimport Pdf from \"react-to-pdf\";\r\nconst ref=React.createRef()\r\n\r\nconst PDF=(props)=>{\r\n    \r\n    return(\r\n        \r\n        <>\r\n            <div  ref={ref}>\r\n                \r\n            <ul><br/><br/>\r\nप्रतिज्ञापत्र\r\n<br/>\r\n\r\nसामाजिक व शैक्षणिकदृष्ट्या मागास (SEBC & Hieght Chest )\r\n<br/>\r\nप्रवर्गाकरिता (मराठा) जात प्रमाणपत्र मिळण्यासाठी अर्जदाराने सादर करावयाचे प्रतिज्ञापत्र ●\r\n<br/>\r\nमा. कार्यकारी दंडाधिकारी सो, तहसील कार्यालय अमळनेर यांचे समोर,\r\n<br/>\r\nदिनांक: 23/07/2021\r\n\r\n<p>मी. श्री. जयेश भरत पाटील, श्री. भरत भास्कर पाटील, यांचा मुलगा वय 21, राहणार : पातोंडा, तालुका :- अमळनेर, जि. जळगाव. \r\n    असे प्रतिज्ञापत्र लिहून देतो की, सन 2018 वा महाराष्ट्र अधिनियम क्रमांक 62 अन्वये सामाजिक व शैक्षणिकदृष्ट्या मागास (सा. शै.मा.प्र.) (SEBC) \r\n    प्रवर्गाकरिता मराठा जात प्रमाणपत्र मिळणेकरिता अर्ज सादर करायचा / केलेला आहे. </p>\r\n\r\n<p>यापुर्वी मी किंवा माझ्या कुटूंबातील इतर कोणत्याही सदस्यांनी \"इतर मागास प्रवर्गातील* OBC * *कुणबी या जातीचे प्रमाणपत्र घेतलेले नाही व अश्या \r\n    प्रमाणपत्रावरून कोणत्याही शासकीय सवलतींचा यापुर्वी लाभ घेतलेला नाही व भविष्यातही घेणार नाही. मी शपथपूर्वक असेही जाहिर करतो की, \r\n    मी वरीलप्रमाणे दिलेली संपुर्ण माहिती खरी व बरोबर आहे.</p>\r\n\r\n<p>मी दिलेली वरील माहिती चुकीची किंवा खोटी आढळुन आल्यास भारतीय दंड संहितेची\r\nकलम 193 (2), 199 व 200 यांच्या तरतुदीनुसार माझ्यावर गुन्हा दाखल होईल व त्यानुसार\r\nहोणाऱ्या शिक्षेस मी पात्र असेन याची मला जाणीव आहे.</p>\r\n\r\n            </ul>\r\n              \r\n              \r\n            \r\n            </div>\r\n\r\n            <div>\r\n            <Pdf targetRef={ref} filename='post.pdf'>\r\n                {({ toPdf })=><button onClick={toPdf} style={{backgroundColor:\"#000080\",color:\"white\"}}>Capture as PDF</button>}\r\n                \r\n            </Pdf>\r\n\r\n            \r\n            </div>\r\n            \r\n        </>\r\n    )\r\n}\r\nexport default PDF;"]},"metadata":{},"sourceType":"module"}