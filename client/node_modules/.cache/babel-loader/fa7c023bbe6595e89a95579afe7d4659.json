S,QAF7F,6EAEuHT,KAAK,CAACG,EAF7H,OAEkIH,KAAK,CAACI,QAFxI,eAIA;AAAA;AAAA;AAAA;AAAA,kBAJA;AAAA;AAAA;AAAA;AAAA;AAAA,gBAjDY,eAwDZ;AAAA;AAAA;AAAA;AAAA,gBAxDY,eAyDZ;AAAG,UAAA,KAAK,EAAE;AACNF,YAAAA,QAAQ,EAAC;AADH,WAAV;AAAA,qDAGSF,KAAK,CAACO,OAHf,eAIA;AAAA;AAAA;AAAA;AAAA,kBAJA,sNAMA;AAAI,YAAA,KAAK,EAAE;AACPY,cAAAA,WAAW,EAAC;AADL,aAAX;AAAA,sBAEInB,KAAK,CAACgB;AAFV;AAAA;AAAA;AAAA;AAAA,kBANA;AAAA;AAAA;AAAA;AAAA;AAAA,gBAzDY,eAoEZ;AAAA;AAAA;AAAA;AAAA,gBApEY,eAqEZ;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,gBArEY,eAsEZ;AAAK,UAAA,GAAG,EAAEhB,KAAK,CAACc,GAAhB;AAAqB,UAAA,KAAK,EAAE;AAACK,YAAAA,WAAW,EAAC;AAAb,WAA5B;AAAkD,UAAA,GAAG,EAAC;AAAtD;AAAA;AAAA;AAAA;AAAA,gBAtEY,eAwEZ;AAAG,UAAA,KAAK,EAAE;AACNjB,YAAAA,QAAQ,EAAC;AADH,WAAV;AAAA,qBAEIF,KAAK,CAACG,EAFV,QAEgBH,KAAK,CAACI,QAFtB,gSAEyFJ,KAAK,CAACe,EAF/F,EAEmGf,KAAK,CAACgB,SAFzG,eAEmH;AAAA;AAAA;AAAA;AAAA,kBAFnH,ugBAIA;AAAA;AAAA;AAAA;AAAA,kBAJA,eAIK;AAAA;AAAA;AAAA;AAAA,kBAJL,2CAOA;AAAA;AAAA;AAAA;AAAA,kBAPA,kCAQShB,KAAK,CAACO,OARf,eASA;AAAA;AAAA;AAAA;AAAA,kBATA,2OAU0L;AAAA;AAAA;AAAA;AAAA,kBAV1L,eAWA;AAAG,YAAA,KAAK,EAAE;AACNN,cAAAA,UAAU,EAAC,OADL;AAENC,cAAAA,QAAQ,EAAC;AAFH,aAAV;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kBAXA,eAiBA;AAAG,YAAA,KAAK,EAAE;AACNA,cAAAA,QAAQ,EAAC;AADH;AAAV;AAAA;AAAA;AAAA;AAAA,kBAjBA,EAmBQF,KAAK,CAACoB,IAnBd,eAuBA;AAAA;AAAA;AAAA;AAAA,kBAvBA,kCAwBSpB,KAAK,CAACO,OAxBf;AAAA;AAAA;AAAA;AAAA;AAAA,gBAxEY,eAiGZ;AAAA;AAAA;AAAA;AAAA,gBAjGY;AAAA;AAAA;AAAA;AAAA;AAAA;AAFA;AAAA;AAAA;AAAA;AAAA,YADJ,eA4GI;AAAA,6BACA,QAAC,GAAD;AAAK,QAAA,SAAS,EAAEV,GAAhB;AAAqB,QAAA,QAAQ,EAAC,UAA9B;AAAA,kBACK,CAAC;AAAEwB,UAAAA;AAAF,SAAD,kBAAa;AAAQ,UAAA,OAAO,EAAEA,KAAjB;AAAwB,UAAA,KAAK,EAAE;AAACC,YAAAA,eAAe,EAAC,SAAjB;AAA2BC,YAAAA,KAAK,EAAC;AAAjC,WAA/B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AADlB;AAAA;AAAA;AAAA;AAAA;AADA;AAAA;AAAA;AAAA;AAAA,YA5GJ;AAAA,kBAFJ;AAyHH,CA3HD;;KAAMxB,G;AA4HN,eAAeA,GAAf","sourcesContent":["\r\nimport React from 'react';\r\n\r\nimport Pdf from \"react-to-pdf\";\r\nconst ref=React.createRef()\r\n\r\nconst PDF=(props)=>{\r\n    \r\n    return(\r\n        \r\n        <>\r\n            <div  ref={ref}>\r\n                \r\n            <ul><br/>\r\n            <p style={{marginLeft:'300px',\r\n        fontSize:'16px'}}> -प्रतिज्ञापत्र-</p>\r\n           \r\n\r\n<br/><h6 style={{marginLeft:'100px',fontSize:'16px'}}>कुटुंबाचे सर्व मानले मिळणारे एकूण वार्षिक उत्पन्न: (दिनांक- 26 ऑक्टोबर 2010 च्या शासन<br/></h6> \r\n<h6 style={{marginLeft:'200px',fontSize:'16px'}}> निर्णयातील  तरतुदी प्रमाणे वार्षिक उत्पन्नाचे प्रमाणपत्र)<br/></h6>\r\n<h6 style={{marginLeft:'150px',fontSize:'16px'}}>मा. कार्यकारी दंडाधिकारी तहसील कार्यालय अमळनेर यांचे समोर,</h6>\r\n<br/>\r\n<p style={{\r\n    fontSize:'16px'\r\n}}>R. 18/06/2021.</p>\r\n\r\n<p style={{\r\n    fontSize:'12px'\r\n}}>मी {props.s1}{props.fullName} , वय: {props.age}, धंदा:- {props.occupation}, रा. {props.village}, ता. {props.taluka}, जि.{props.district} सत्य प्रतिशेवर लिहून देतो कि मी वरील <br/> \r\nगावाच्या कायमस्वरूपी रहिवासी असून, सन 2002-2007 दारिद्र्य रेषेतील कुटुंब यादीतील माझे नाव अनुक्रमांक- {props.daridrano} वर असून / नसून मला -गुण \r\nमिळालेले आहेत नाहीत.<br/> मी स्वतः कुटुंब प्रमुख वरील कुटुंबातील सदस्य आहे. मी सन 2020-2021 या वर्षांचा मा. तहसीलदार \r\n यांचेकडील {props.utpan}/- उत्पन्न दाखला जोडलेला आहे.</p>\r\n\r\n <p style={{\r\n    fontSize:'12px'\r\n}}>माझी जन्मतारीख:- 01/03/1948 असून वयाचा पुरावा म्हणून मी {props.ageproof} सोबत जोडलेला आहे{props.lc}. आज रोजी  \r\nमाझे वय {props.age} वर्ष आहे.</p>\r\n\r\n<p style={{\r\n    fontSize:'12px'\r\n}}>तसेच माझ्या कुटुंबात कोणीही शासकीय, निमशासकीय व खाजगी नोकरीला नाही व पेन्शन नाही आजतागायत मी कुठल्याही प्रकारची आर्थिक मदत येत नाही\r\n किंवा <br/> घेतलेली नाही  मला या पुढील वृद्ध अवस्थेतील जीवन जगण्यासाठी किंवा माझा उदरनिर्वाह  करण्यासाठी मला उत्पन्नावे  कोणतेही साधन\r\n  जाठी मी अशिक्षित / कमी शिक्षण  असून <br/>माझ्या वयाबाबत पुरावा म्हणून आधार कार्ड व शाळा सोडल्याचा दाखला हजर केलेला आहे. तसेच माझे कुटूंबाचे  एकत्रित उत्पन्न {props.utpan} इतके आहे.</p>\r\n\r\n <p style={{\r\n    fontSize:'12px'\r\n}}>सदरचे प्रतिज्ञापत्र हे ।) श्रावण बाळ सेवा राज्य निवृत्तीवेतन योजना, 2) इंदिरा गांधी राष्ट्रीयवृद्धापकाळ निवृत्तीवेतन योजना,3) इंदिरा गांधी राष्ट्रीय विधवा निवृत्तीवेतन योजना,<br/>\r\n 4) इंदिरा गांधी राष्ट्रीय अपंग निवृत्तीवेतन योजना, 5) संजय गांधी विधवा निवृत्तीवेतन योजना,6) संजय गांधी अपंग निवृत्तीवेतन योजनो अंतर्गर्त लाभ मिळणेकमी करून दिले <br/>असून ते सत्य व बरोबर आहे <br/> व तवाडीचे  निघाल्यास यातील \r\nकोणतीही माहिती चुकीची आढळल्यास त्यास मी स्वतः जबाबदार राहीन.</p>\r\n\r\n<p style={{\r\n    fontSize:'12px'\r\n}}>मी असे जाहीर करतो/करते की, मी वर दिलेली असून ते सत्य व बिनचूक असून मी शासनाच्या अन्य कोणत्याही योजनेखाली <br/> नियमित\r\n    आर्थिक लाभ घेत नाही कुटुंबातील कोणीही नोकरीवर नाही. सदस्ती माहिती चुकीची आढळकतयास माझ्या विरुद्ध  <br/> भा.द.वि. कलम 199 व 200(2) \r\n    नुसार फौजदारी शिक्षेस व दंडास मी पात्र राहील याची मला संपूर्ण जाणीव आहे.</p>\r\n\r\n<img src={props.img} alt=\"pic\"/>\r\n\r\n\r\n\r\n    <h5>साक्षीदाराचे नांव व पत्ता :</h5>\r\n<br/>\r\n<p style={{\r\n    fontSize:'12px'\r\n}}>मी. {props.s3}{props.fullname2}   रा. {props.village1},  ता. {props.taluka1},  जि. {props.district}. माझ्या समक्ष  {props.s1} {props.fullName}\r\n\r\n<br/> यांनी सही व अंगठ्याचा ठसा दिलेला आहे व त्यांना मी वैक्तिकरित्या ओळखतो असे मी घोषित करतो.</p>\r\n\r\n\r\n<br/>\r\n<p style={{\r\n    fontSize:'12px'\r\n}}>\r\nस्थळ :- {props.village}       \r\n<br/>                      \r\nदिनांक :- 20/09/2021.                &emsp;&emsp;&emsp;&emsp; &emsp;&emsp;&emsp; &emsp;&emsp;    &emsp;&emsp; &emsp;&emsp; &emsp;&emsp;&emsp; &emsp;&emsp;     (Sign)\r\n<h5 style={{\r\n    paddingLeft:'450px'\r\n}}>{props.fullname2}</h5>\r\n</p>\r\n\r\n<br/>\r\n<h5>प्रमाणित करण्यात येते कि :</h5>\r\n<img src={props.img} style={{paddingLeft:'450px'}}alt=\"pic\"/>\r\n\r\n<p style={{\r\n    fontSize:'12px'\r\n}}>{props.s1}  {props.fullName} यांनी स्वाक्षरी केली आणि माझ्या समक्ष शपथ घेतली. यांना, {props.s3}{props.fullname2}<br/>\r\n  यांनी वैयक्तिकरित्या ओळखले आहे. ज्यांची स्वाक्षरी प्रतिज्ञापत्रा वर माझ्या समक्ष केली गेली आहे.\r\n<br/><br/>\r\n\r\nRegister No. MH-01/2021.\r\n<br/>\r\nस्थळ :- {props.village}       \r\n<br/>                      \r\nदिनांक :- 20/09/2021.         &emsp;&emsp; &emsp;&emsp; &emsp;&emsp;&emsp; &emsp;&emsp;     (Seal of office)          &emsp;&emsp;&emsp;&emsp; &emsp;&emsp;             (Sign. Before Me) <br/>\r\n<p style={{\r\n    marginLeft:'470px',\r\n    fontSize:'12px'\r\n}}>  कार्यकारी दंडाधिकारी अमळनेर</p>\r\n\r\n\r\n<p style={{\r\n    fontSize:'12px'\r\n}}></p>{props.sign}\r\n\r\n\r\n\r\n<br/>\r\nस्थळ :- {props.village}</p>\r\n<br/>      \r\n\r\n            </ul>\r\n              \r\n              \r\n            \r\n            </div>\r\n\r\n            <div>\r\n            <Pdf targetRef={ref} filename='post.pdf'>\r\n                {({ toPdf })=><button onClick={toPdf} style={{backgroundColor:\"#000080\",color:\"white\"}}>Capture as PDF</button>}\r\n                \r\n            </Pdf>\r\n\r\n            \r\n            </div>\r\n            \r\n        </>\r\n    )\r\n}\r\nexport default PDF;"]},"metadata":{},"sourceType":"module"}