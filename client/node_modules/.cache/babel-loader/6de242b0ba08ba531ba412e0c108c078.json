;AAAA;AAAA;AAAA,gBAJjH,6IAKU;AAAA;AAAA;AAAA;AAAA,gBALV,kUAOZ;AAAA;AAAA;AAAA;AAAA,gBAPY,eAOP;AAAA;AAAA;AAAA;AAAA,gBAPO,iCASZ;AAAA;AAAA;AAAA;AAAA,gBATY,eASP;AAAA;AAAA;AAAA;AAAA,gBATO,eAUZ;AAAA,kiBAAoH;AAAA;AAAA;AAAA;AAAA,kBAApH,4iBACgH;AAAA;AAAA;AAAA;AAAA,kBADhH,kiBAE6F;AAAA;AAAA;AAAA;AAAA,kBAF7F;AAAA;AAAA;AAAA;AAAA;AAAA,gBAVY,eAeZ;AAAA,ggBAA2G;AAAA;AAAA;AAAA;AAAA,kBAA3G;AAAA;AAAA;AAAA;AAAA;AAAA,gBAfY,eAiBZ;AAAA,6jBAA4G;AAAA;AAAA;AAAA;AAAA,kBAA5G,yvBACkH;AAAA;AAAA;AAAA;AAAA,kBADlH,usBAEsG;AAAA;AAAA;AAAA;AAAA,kBAFtG;AAAA;AAAA;AAAA;AAAA;AAAA,gBAjBY,eAqBZ;AAAA,qqBAA+H;AAAA;AAAA;AAAA;AAAA,kBAA/H,onBAC4H;AAAA;AAAA;AAAA;AAAA,kBAD5H,gpBAE2G;AAAA;AAAA;AAAA;AAAA,kBAF3G;AAAA;AAAA;AAAA;AAAA;AAAA,gBArBY,eA0BZ;AAAA,ojBAA6G;AAAA;AAAA;AAAA;AAAA,kBAA7G,qjBACsG;AAAA;AAAA;AAAA;AAAA,kBADtG;AAAA;AAAA;AAAA;AAAA;AAAA,gBA1BY;AAAA;AAAA;AAAA;AAAA;AAAA;AAFA;AAAA;AAAA;AAAA;AAAA,YADJ,eAwCI;AAAA,6BACA,QAAC,GAAD;AAAK,QAAA,SAAS,EAAEA,GAAhB;AAAqB,QAAA,QAAQ,EAAC,UAA9B;AAAA,kBACK,CAAC;AAAEI,UAAAA;AAAF,SAAD,kBAAa;AAAQ,UAAA,OAAO,EAAEA,KAAjB;AAAwB,UAAA,KAAK,EAAE;AAACC,YAAAA,eAAe,EAAC,SAAjB;AAA2BC,YAAAA,KAAK,EAAC;AAAjC,WAA/B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AADlB;AAAA;AAAA;AAAA;AAAA;AADA;AAAA;AAAA;AAAA;AAAA,YAxCJ;AAAA,kBAFJ;AAqDH,CAvDD;;KAAMJ,G;AAwDN,eAAeA,GAAf","sourcesContent":["\r\nimport React from 'react';\r\n\r\nimport Pdf from \"react-to-pdf\";\r\nconst ref=React.createRef()\r\n\r\nconst PDF=(props)=>{\r\n    \r\n    return(\r\n        \r\n        <>\r\n            <div  ref={ref}>\r\n                \r\n            <ul><br/><br/>\r\n            -प्रतिज्ञापत्र-\r\n            <br/>\r\n\r\n<br/>कुटुंबाचे सर्व मानले मिळणारे एकूण वार्षिक उत्पन्न: (दिनांक- 26 ऑक्टोबर 2010 च्या शासन निर्णयातील तरतुदी प्रमाणे वार्षिक <br/>\r\nउत्पन्नाचे प्रमाणपत्र)<br/>\r\nमा. कार्यकारी दंडाधिकारी तहसील कार्यालय अमळनेर यांचे समोर,\r\n<br/><br/>\r\nR. 18/06/2021.\r\n<br/><br/>\r\n<p>मी श्री. अभिमन महीपत चांभार (सोनवणे), वय: 73, धंदा:- हातमजुरी, रा. पातोंडा, ता. अमळनेर, जि. जळगाव सत्य प्रतिशेवर <br/>\r\nलिहून देतो कि मी वरील गावाच्या कायमस्वरूपी रहिवासी असून, सन 2002-2007 दारिद्र्य रेषेतील कुटुंब यादीतील माझे नाव <br/>अनुक्रमांक- वर   \r\n असून / नसून मला -गुण मिळालेले आहेत नाहीत. मी स्वतः कुटुंब प्रमुख वरील कुटुंबातील सदस्य आहे. <br/>मी सन 2020-2021 या वर्षांचा मा. तहसीलदार \r\n यांचेकडील 21000/- उत्पन्न दाखला जोडलेला आहे.</p>\r\n\r\n<p>माझी जन्मतारीख:- 01/03/1948 असून वयाचा पुरावा म्हणून मी शाळा सोडल्याचा दाखता सोबत जोडलेला आहे. आज रोजी  <br/> माझे वय 73 वर्ष आहे.</p>\r\n\r\n<p> तसेच माझ्या कुटुंबात कोणीही शासकीय, निमशासकीय व खाजगी नोकरीला नाही व पेन्शन नाही आजतागायत मी कुठल्याही  <br/>प्रकारची आर्थिक मदत येत नाही\r\n किंवा घेतलेली नाही मला या पुढील वृद्ध अवस्थेतील जीवन जगण्यासाठी किंवा माझा उदरनिर्वाह करण्यासाठी मला उत्पन्नावे  <br/>कोणतेही साधन जाठी मी अशिक्षित / कमी\r\n शिक्षण असून माझ्या वयाबाबत पुरावा म्हणून आधार कार्ड व शाळा सोडल्याचा दाखला हजर केलेला आहे. तसेच माझे <br/>कुटूंबाचे  एकत्रित उत्पन्न 21000/- इतके आहे.</p>\r\n\r\n<p>सदरचे प्रतिज्ञापत्र हे ।) श्रावण बाळ सेवा राज्य निवृत्तीवेतन योजना, 2) इंदिरा गांधी राष्ट्रीयवृद्धापकाळ निवृत्तीवेतन योजना, <br/>\r\n3) इंदिरा गांधी राष्ट्रीय विधवा निवृत्तीवेतन योजना, 4) इंदिरा गांधी राष्ट्रीय अपंग निवृत्तीवेतन योजना, 5) संजय गांधी विधवा  <br/>निवृत्तीवेतन योजना, \r\n6) संजय गांधी अपंग निवृत्तीवेतन योजनो अंतर्गर्त लाभ मिळणेकमी करून दिले असून ते सत्य व बरोबर आहे व तवाडीचे  <br/> निघाल्यास यातील \r\nकोणतीही माहिती चुकीची आढळल्यास त्यास मी स्वतः जबाबदार राहीन.</p>\r\n\r\n<p> मी असे जाहीर करतो/करते की, मी वर दिलेली असून ते सत्य व बिनचूक असून मी शासनाच्या अन्य कोणत्याही योजनेखाली <br/> नियमित\r\n    आर्थिक लाभ घेत नाही कुटुंबातील कोणीही नोकरीवर नाही. सदस्ती माहिती चुकीची आढळकतयास माझ्या विरुद्ध  <br/> भा.द.वि. कलम 199 व 200(2) \r\n    नुसार फौजदारी शिक्षेस व दंडास मी पात्र राहील याची मला संपूर्ण जाणीव आहे.</p>\r\n      \r\n\r\n            </ul>\r\n              \r\n              \r\n            \r\n            </div>\r\n\r\n            <div>\r\n            <Pdf targetRef={ref} filename='post.pdf'>\r\n                {({ toPdf })=><button onClick={toPdf} style={{backgroundColor:\"#000080\",color:\"white\"}}>Capture as PDF</button>}\r\n                \r\n            </Pdf>\r\n\r\n            \r\n            </div>\r\n            \r\n        </>\r\n    )\r\n}\r\nexport default PDF;"]},"metadata":{},"sourceType":"module"}